{
  "AWSEBDockerrunVersion": 2,
  "authentication": {
    "Bucket": "mmdp-devops",
    "KEY": "config.json"
  },
  "volumes": [],
  "containerDefinitions": [
    {
      "name": "client",
      "image": "shemogumbe/mmdp_client",
      "hostname": "client",
      "essential": false,
      "links": [
        "server"
      ],
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 1234
        }
      ],
      "memory": 200
    },
    {
      "name": "server",
      "image": "shemogumbe/mmdp_api",
      "hostname": "api",
      "essential": false,
      "portMappings": [
        {
          "hostPort": 3000,
          "containerPort": 3000
        }
      ],
      "memory": 512
    }
  ]
}
